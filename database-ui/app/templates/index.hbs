<style>
table {
	height: 400px;
	width: 400px;
	margin: 0; padding: 0;
	border-collapse: collapse;
}
td {
	border: 1px solid #CC3;
	border-spacing: 0;
	height: 100px;
	width: 100px;
	margin: 0; padding: 0;
}
</style>
<div>
	{{#each getTables as |table|}}
		<p>{{table}}</p>
	{{/each}}
	{{getTables}}
	<datalist id="tables">
		{{#each getTables as |table|}}
    	{{option value=table}}
		{{/each}}
  </datalist>
  <br/>
  <b>Enter the table_name:</b><br/>
  {{input list="tables" type="text" id="table_name" name="table_name" value=table_name placeholder="Enter table_name" }}
  <button {{action "display"}}>Go</button>
</div>
<center><br/><br/>
{{message}}
<div id="tablediv">
{{#table-pagination paginateBy=5 sortProperties = sortProperties sortAscending = sortAscending prop = prop table_name = table_name cols=model.col items=model.row as |rows| }}
	{{#disp-table colData=model.col rowData=rows table_name=table_name sortBy = 'sortBy'}}
	{{/disp-table}}
{{/table-pagination}}
</div>
</center>
