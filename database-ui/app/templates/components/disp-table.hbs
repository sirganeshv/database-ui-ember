<style>
td,th {
    border: 1px solid black;
    font-size: 25px;
    align: center;
    min-width:70px;
}
table {
  border-collapse: collapse;
  padding: 15px;
  table-layout: auto;
  width: 180px;
}
</style>
<table id='table'>
  <thead id="tablehead">
    <tr>
      {{#each data.col as |col|}}
      <th width = 5%>
        {{col}}
      </th>
      {{/each}}
    </tr>
  </thead>
  <tbody id = "tablebody">
    {{#each data.row as |row|}}
      <tr>
        {{#each row as |rowData|}}
          <td align="center">
            {{rowData}}
          </td>
        {{/each}}
      </tr>
    {{/each}}
  </tbody>
</table>
